#!/bin/bash  -l
#
# Generation of the input file for trove energies
#


L=$1
T=$2
Jmax=$3
name=$4

h=0.005
fname=95ma_08brhaho_HS_int_L${L}_J${Jmax}_$name
duo=/scratch/sheena_abigail/j-duo-v0911.x

echo $fname

N=`echo $L $h | awk  '{printf( "%8.0f\n", ($1-0.5)/$2+1 )}'`

echo "N =" $N


cat<<endb> $fname.inp
(SH-Davidson corrected energies, as grid)

atoms S H

molecule SH

(Total number of states taken into account)
nstates 6

(Total angular momentum quantum  - a value or an interval)
jrot 1.5, 0.5  2.5 - 10.5

PRINT_PECS_AND_COUPLINGS_TO_FILE

(Defining the integration grid)

grid
  npoints $N  (odd)
  range  0.85,$L
  type 0   (nsub)
end 


symmetry Cs(M)


DIAGONALIZER 
 SYEV
end


CONTRACTION
  vib
  vmax 40 10  $N $N $N 
  enermax 40000
END

